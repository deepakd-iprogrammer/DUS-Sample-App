{
  "name": "dus-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "setup": "npm install -g dus-deployer",
    "serverStart": "node server.js",
    "update": "npm run deploy2 && node deployUpdatePatch.js",
    "deploy2": "dus-deployer --config DeploymentConfig-2.json --platform android --react16 true --updateGraphVersion 0.0.0.1 --outputPath output",
    "release": "npm run deploy1 && node deployUpdatePatch.js",
    "deploy1": "cd DeploymentConfigs && dus-deployer --config DeploymentConfig.json --platform android --react16 true --updateGraphVersion 0.0.0.0 --outputPath output"
  },
  "dependencies": {
    "dus-deployer": "^1.47.6",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.2"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "body-parser": "1.18.2",
    "express": "4.16.2",
    "node-fetch": "^1.7.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
